{"ast":null,"code":"import Axios from '@/services/trylogin';\nexport default {\n  name: \"LoginPage\",\n\n  data() {\n    return {\n      userLogin: {\n        //dados que vão para o servidor\n        email: \"\",\n        senha: \"\"\n      },\n      dadosBack: null,\n      //recebe os dados do back\n      encaminhamento: {\n        token: \"\",\n        acesso: \"\"\n      }\n    };\n  },\n\n  methods: {\n    async tryLogin(e) {\n      e.preventDefault(); //metodo Post com os dados do userLogin\n\n      Axios.save(this.userLogin).then(resposta => {\n        this.dadosBack = resposta.data; //recebendo os valores que o post gerou\n\n        for (let elemento of this.dadosBack) {\n          this.encaminhamento.acesso = elemento.acesso;\n        }\n\n        for (let elemento of this.dadosBack) {\n          this.encaminhamento.token = elemento.token;\n        }\n\n        console.log(this.encaminhamento.acesso);\n        console.log(this.encaminhamento.token);\n        this.$store.dispatch(\"saveToken\", this.encaminhamento.token);\n        /* if (this.encaminhamento.acesso === \"A\") {\r\n              window.location.href = \"http://localhost:8080/pai\"\r\n          } else if (this.encaminhamento.acesso === \"P\") {\r\n              window.location.href = \"http://localhost:8080/prof\"\r\n          } else {\r\n              alert(\"Login ou Senha Invalidos\")\r\n          } */\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAsNA,OAAOA,KAAP,MAAkB,qBAAlB;AAEA,eAAe;EAEXC,IAAI,EAAE,WAFK;;EAGXC,IAAI,GAAE;IACF,OAAM;MACFC,SAAS,EAAE;QAAE;QACTC,KAAK,EAAE,EADA;QAEPC,KAAK,EAAE;MAFA,CADT;MAMFC,SAAS,EAAE,IANT;MAMgB;MAClBC,cAAc,EAAE;QACZC,KAAK,EAAE,EADK;QAEZC,MAAM,EAAE;MAFI;IAPd,CAAN;EAeH,CAnBU;;EAoBXC,OAAO,EAAE;IACL,MAAMC,QAAN,CAAeC,CAAf,EAAkB;MACdA,CAAC,CAACC,cAAF,GADc,CAIjB;;MACAb,KAAK,CAACc,IAAN,CAAW,KAAKX,SAAhB,EAA2BY,IAA3B,CAAgCC,QAAO,IAAI;QACpC,KAAKV,SAAL,GAAiBU,QAAQ,CAACd,IAA1B,CADoC,CACL;;QAG/B,KAAI,IAAIe,QAAR,IAAoB,KAAKX,SAAzB,EAAmC;UAChC,KAAKC,cAAL,CAAoBE